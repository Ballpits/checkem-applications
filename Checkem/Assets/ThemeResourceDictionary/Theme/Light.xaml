<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="HighlightColor" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="TransparentHighlightColor" Color="#4BFFFFFF"/>


    <SolidColorBrush x:Key="Color.White" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="Color.WhiteSmoke" Color="WhiteSmoke"/>

    <SolidColorBrush x:Key="Color.LightGray" Color="#FFE6E6E6"/>
    <SolidColorBrush x:Key="Color.Gray" Color="#FFCBCBCB"/>

    <SolidColorBrush x:Key="Color.Black" Color="Black"/>
</ResourceDictionary>