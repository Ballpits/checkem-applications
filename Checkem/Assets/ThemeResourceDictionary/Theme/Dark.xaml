<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="HighlightColor" Color="Black"/>
    <SolidColorBrush x:Key="TransparentHighlightColor" Color="#4BFFFFFF"/>

    <Color x:Key="WhiteColor">#FF000000</Color>

    <SolidColorBrush x:Key="Color.White" Color="{DynamicResource WhiteColor}"/>
    <SolidColorBrush x:Key="Color.WhiteSmoke" Color="#FF0A0A0A"/>

    <SolidColorBrush x:Key="Color.LightGray" Color="#191919"/>
    <SolidColorBrush x:Key="Color.Gray" Color="#343434"/>
    <SolidColorBrush x:Key="Color.DarkGray" Color="DarkGray"/>

    <SolidColorBrush x:Key="Color.Black" Color="White"/>
</ResourceDictionary>