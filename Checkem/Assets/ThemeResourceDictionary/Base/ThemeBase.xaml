<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >

    <Color x:Key="Color.White">#FFFFFFFF</Color>
    <Color x:Key="Color.Black">#FF000000</Color>

    <SolidColorBrush x:Key="SolidColorBrush.White.Constant" Color="{StaticResource Color.White}"/>
    <SolidColorBrush x:Key="SolidColorBrush.Black.Constant" Color="{StaticResource Color.Black}"/>


    <Color x:Key="Color.Warning">#FFEC5433</Color>
    <Color x:Key="Color.Strikethrough">#FF969696</Color>
    <Color x:Key="Color.Transparent">#00000000</Color>
    
    <SolidColorBrush x:Key="SolidColorBrush.Warning" Color="{StaticResource Color.Warning}"/>
    <SolidColorBrush x:Key="SolidColorBrush.Strikethrough" Color="{StaticResource Color.Strikethrough}"/>

    <SolidColorBrush x:Key="SolidColorBrush.Transparent" Color="{StaticResource Color.Transparent}"/>
</ResourceDictionary>