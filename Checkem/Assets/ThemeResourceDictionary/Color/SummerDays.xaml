<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >


    <Color x:Key="PrimaryColor">#FFA8FF78</Color>
    <Color x:Key="SecondaryColor">#FF78FFD9</Color>

    <Color x:Key="PrimaryHighlightColor">#7BA8FF78</Color>
    <Color x:Key="SecondaryHighlightColor">#FB78FFD9</Color>


    <SolidColorBrush x:Key="Color.Primary" Color="{DynamicResource PrimaryColor}"/>
    <SolidColorBrush x:Key="Color.Secondary" Color="{DynamicResource SecondaryColor}"/>

    <SolidColorBrush x:Key="Color.TextOnPrimaryColor" Color="{DynamicResource Color.Black.Constant}"/>

    <SolidColorBrush x:Key="HighlightColor.Primary" Color="{DynamicResource PrimaryHighlightColor}"/>
    <SolidColorBrush x:Key="HighlightColor.Secondary" Color="{DynamicResource SecondaryHighlightColor}"/>
</ResourceDictionary>