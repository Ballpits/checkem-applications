<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <SolidColorBrush x:Key="HighlightColor" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="TransparentHighlightColor" Color="#4BFFFFFF"/>

    <Color x:Key="WhiteColor">#FFFFFFFF</Color>

    <SolidColorBrush x:Key="Color.White" Color="{DynamicResource WhiteColor}"/>
    <SolidColorBrush x:Key="Color.WhiteSmoke" Color="WhiteSmoke"/>

    <SolidColorBrush x:Key="Color.LightGray" Color="#FFE6E6E6"/>
    <SolidColorBrush x:Key="Color.Gray" Color="#7FCBCBCB"/>
    <SolidColorBrush x:Key="Color.DarkGray" Color="DarkGray"/>

    <SolidColorBrush x:Key="Color.Black" Color="Black"/>
</ResourceDictionary>